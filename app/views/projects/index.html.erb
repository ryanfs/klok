<h1>This is a list of projects!</h1>

<% @projects.each do |project| %>

  <p style="color: <%= project.hex %>"><%= project.name %></p>
  <hr />

  <% project.timeblocks.each do |timeblock| %>
    <p>Start: <%= timeblock.start %></p>
    <p>Stop: <%= timeblock.end %></p>
    <p>Description: <%= timeblock.description %></p>
    <hr />
  <% end  %>

<% end %>


